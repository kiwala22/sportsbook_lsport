<% fixtures.each do |fixture| %>
      <tr data-controller="pre-odds-channel #markets-channel slips" data-pre-odds-channel-fixture=<%= fixture.id %> data-markets-channel-fixture=<%= fixture.id %> data-slips-fixture=<%= fixture.id %> data-pre-odds-channel-market="1" data-markets-channel-market="1" data-slips-market="1" >
         <td>
            <a href=<%=fixtures_soccer_pre_path(id: fixture.id) %>>
               <%= local_time(fixture.scheduled_time, format: '%H:%M:%S') %> <br>
               <%= local_time(fixture.scheduled_time, format: '%d/%m/%y') %>
            </a>
         </td>
         <td>
            <a id=<%= "show_#{fixture.id}" %> href=<%=fixtures_soccer_pre_path(id: fixture.id) %>>
            <strong><%= fixture.comp_one_name %></strong>
            <strong><%= fixture.comp_two_name %></strong>
            </a>
         </td>
         <td>
            <a href=<%=fixtures_soccer_pre_path(id: fixture.id) %>>
               <%= fixture.tournament_name %> <br>
               <%= fixture.category %>
            </a>
         </td>
         <td>
            <%= link_to(fixture.market1_pre.outcome_1, add_bet_path(outcome_id: "1", fixture_id: fixture.id, market: "Market1Pre", outcome_desc: "1X2 FT - 1") , class: "btnn intialise_input", id: "pre_1_#{fixture.id}", remote: true, method: :post, data: {disable_with: "<i class='fas fa-spinner fa-spin'></i>"})%>
         </td>
         <td>
            <%= link_to(fixture.market1_pre.outcome_2, add_bet_path(outcome_id: "2", fixture_id: fixture.id, market: "Market1Pre", outcome_desc: "1X2 FT - X") , class: "btnn intialise_input", id: "pre_2_#{fixture.id}",remote: true, method: :post, data: {disable_with: "<i class='fas fa-spinner fa-spin'></i>"} )%>
         </td>
         <td>
            <%= link_to(fixture.market1_pre.outcome_3, add_bet_path(outcome_id: "3", fixture_id: fixture.id, market: "Market1Pre", outcome_desc: "1X2 FT - 2") , class: "btnn intialise_input", id: "pre_3_#{fixture.id}", remote: true, method: :post, data: {disable_with: "<i class='fas fa-spinner fa-spin'></i>"} )%>
         </td>
      </tr>
<% end %>
