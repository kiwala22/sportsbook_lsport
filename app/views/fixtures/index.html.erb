            <div class="tab-content" id="myTabContent">
                <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                    <table class="table table-hover table-borderless table-striped fixture-table">
                        <thead>
                            <tr>
                                <th class="col-2">Date</th>
                                <th class="col-4">Teams</th>
                                <th class="col-2">1</th>
                                <th class="col-2">3 Way <br>X</th>
                                <th class="col-2">2</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% @fixtures.each do |fixture| %>
                                <% if fixture.market1_pre.present? %>
                                    <tr>
                                        <td class= "col-2"><%= fixture.scheduled_time.strftime("%H:%M:%S") %> <br> <%= fixture.scheduled_time.strftime("%d/%m/%y") %></td>
                                        <td class= "col-4"><strong> <%= fixture.comp_one_name %> </strong> <strong> <%= fixture.comp_two_name %> </strong></td>
                                        <td class= "col-2"><%= fixture.market1_pre.competitor1 %></td>
                                        <td class= "col-2"><%= fixture.market1_pre.draw %></td>
                                        <td class= "col-2"><%= fixture.market1_pre.competitor2 %></td>
                                    </tr>
                                <% end %>
                            <% end %>
                        </tbody>
                    </table>
                </div>
